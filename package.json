{
  "name": "uniswap-sdk",
  "version": "1.0.0",
  "description": "Simple easy to understand SDK for uniswap",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "gen-erc20-abi": "abi-types-generator ./src/ABI/erc-20-abi.json --output=./src/ABI/types --name=erc20-contract --provider=ethers_v5",
    "gen-uniswap-router-v2-abi": "abi-types-generator ./src/ABI/uniswap-router-v2.json --output=./src/ABI/types --name=uniswap-router --provider=ethers_v5",
    "gen-uniswap-factory-v2-abi": "abi-types-generator ./src/ABI/uniswap-factory-v2.json --output=./src/ABI/types --name=uniswap-factory --provider=ethers_v5",
    "gen-uniswap-pair-v2-abi": "abi-types-generator ./src/ABI/uniswap-pair-v2.json --output=./src/ABI/types --name=uniswap-pair --provider=ethers_v5",
    "gen-all-contract-types": "npm run gen-erc20-abi && npm run gen-uniswap-router-v2-abi && npm run gen-uniswap-factory-v2-abi && npm run gen-uniswap-pair-v2-abi"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshstevens19/uniswap-sdk.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joshstevens19/uniswap-sdk/issues"
  },
  "homepage": "https://github.com/joshstevens19/uniswap-sdk#readme",
  "dependencies": {
    "bignumber.js": "^9.0.1",
    "ethereum-multicall": "^2.0.1",
    "ethers": "^5.0.26"
  },
  "devDependencies": {
    "ethereum-abi-types-generator": "^1.0.10"
  }
}
